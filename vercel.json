{
    "functions": {
      "backend/**/*.py": {
        "runtime": "python3.9"  
      }
    },
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/$1"
      }
    ]
  }
  