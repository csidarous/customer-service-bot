{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      },
      {
        "src": "backend/main.py",
        "use": "@vercel/python"
      }

    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/backend/main.py"
      }
    ]
  }
  